{
  "homepage": "https://benoithuvelle.github.io/electricad",
  "name": "electricad",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@thi.ng/geom-closest-point": "^0.3.8",
    "@types/node": "^12.11.7",
    "closest-point-detector": "^1.0.2",
    "d3-shape": "^1.3.5",
    "gh-pages": "^2.1.1",
    "pages": "0.0.16",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-draggable": "^4.0.3",
    "react-scripts": "3.2.0",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss}\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 88,
    "trailingComma": "all",
    "tabWidth": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dom": "^16.9.3",
    "prettier": "^1.18.2"
  }
